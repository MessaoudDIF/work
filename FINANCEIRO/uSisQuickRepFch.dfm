  object quQuickInfo: TADODataSet
    Connection = DM.DBADOConnection
    CursorType = ctStatic
    CommandText = 
      'SELECT'#13#10#9'PO.IDPO,'#13#10#9'PO.OrderDate,'#13#10#9'V.FullName as Vendor,'#13#10#9'S.St' +
      'ore,'#13#10#9'SU.SystemUser,'#13#10#9'PIM.Qty,'#13#10#9'(PIM.Qty * PIM.CostPrice) as ' +
      'ExtCostPrice,'#13#10#9'(PIM.Qty * PIM.SalePrice) as ExtSalePrice,'#13#10#9'(PI' +
      'M.Qty * PIM.SuggRetail) as ExtSuggRetail,'#13#10#9'M.Model,'#13#10#9'M.Descrip' +
      'tion,'#13#10#9'C.Category'#13#10'FROM'#13#10#9'vw_Rep_PurchaseOrder PO'#13#10#9'JOIN vw_Rep' +
      '_Entity V ON (PO.IDVendor = V.IDPerson)'#13#10#9'JOIN vw_Rep_Store S ON' +
      ' (S.IDStore = PO.IDStore)'#13#10#9'JOIN vw_Rep_PreInventoryMov PIM ON (' +
      'PO.IDPO = PIM.IDDocument AND PIM.IDInventMovType = 2)'#13#10#9'LEFT OUT' +
      'ER JOIN vw_Rep_SystemUser SU ON ( PIM.IDUser = SU.IDUser )'#13#10#9'JOI' +
      'N vw_Rep_Model M ON (M.IDModel = PIM.IDModel)'#13#10#9'JOIN vw_Rep_Cate' +
      'gory C ON (M.GroupID = C.IDCategory)'#13#10'ORDER BY '#13#10#9'Vendor, PO.IDP' +
      'O'
    CommandTimeout = 180
    Parameters = <>
    Left = 45
    Top = 75
  end
